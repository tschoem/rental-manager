{
  "name": "rental_manager",
  "version": "0.1.0",
  "description": "An open-source Next.js application to host your Airbnb rooms and properties yourself. Re-take ownership of your properties by building a dedicated website with a simple CMS, calendar synchronization, and direct booking capabilities.",
  "private": true,
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/tschoem/rental-manager.git"
  },
  "homepage": "https://github.com/tschoem/rental-manager#readme",
  "bugs": {
    "url": "https://github.com/tschoem/rental-manager/issues"
  },
  "keywords": [
    "rental",
    "airbnb",
    "property-management",
    "booking",
    "nextjs",
    "cms",
    "calendar-sync",
    "open-source"
  ],
  "scripts": {
    "dev": "tsx scripts/update-schema-provider.ts && prisma generate && tsx scripts/copy-prisma-engine.ts && next dev",
    "build": "tsx scripts/update-schema-provider.ts && prisma generate && tsx scripts/copy-prisma-engine.ts && next build",
    "start": "next start",
    "lint": "eslint",
    "postinstall": "tsx scripts/update-schema-provider.ts && prisma generate && tsx scripts/copy-prisma-engine.ts",
    "vercel-build": "tsx scripts/update-schema-provider.ts && prisma generate && tsx scripts/copy-prisma-engine.ts && next build",
    "setup": "tsx scripts/setup.ts",
    "setup-admin": "tsx scripts/setup-admin.ts",
    "update-property-admin": "tsx scripts/update-property-admin.ts",
    "list-users": "tsx scripts/list-users.ts",
    "delete-user": "tsx scripts/delete-user.ts",
    "list-gemini-models": "tsx scripts/list-gemini-models.ts"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@next-auth/prisma-adapter": "^1.0.7",
    "@prisma/client": "^6.19.0",
    "@types/react-easy-crop": "^1.16.0",
    "axios": "^1.13.2",
    "bcryptjs": "^3.0.3",
    "cheerio": "^1.1.2",
    "next": "16.0.3",
    "next-auth": "^4.24.13",
    "node-ical": "^0.22.1",
    "nodemailer": "^7.0.11",
    "puppeteer": "^24.31.0",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "react-easy-crop": "^5.5.6"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/node": "^20",
    "@types/nodemailer": "^7.0.4",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "baseline-browser-mapping": "^2.8.32",
    "dotenv": "^17.2.3",
    "eslint": "^9",
    "eslint-config-next": "16.0.3",
    "prisma": "^6.19.0",
    "tsx": "^4.20.6",
    "typescript": "^5"
  }
}